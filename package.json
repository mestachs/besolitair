{
  "name": "besolitair",
  "version": "0.0.0",
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "c8 -r html -r text uvu",
    "testw": "watchlist src -- c8 -r html -r text uvu",
    "lint": "prettier --check ./src",
    "format": "prettier --write ./src"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "c8": "^7.7.2",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "uvu": "0.5.1",
    "vite": "^2.2.3",
    "watchlist": "^0.2.3"
  }
}
